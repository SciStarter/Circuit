<!doctype html>
<html>
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/api/docs/bulma.css">
    <link rel="stylesheet" href="/api/docs/manage.css">
    <title>Partner health summary</title>
  </head>
  <body>
    <section class="section">
      <div class="container">
        <a href="..">Back to manage</a>
        <h1 class="title">Partner health summary</h1>

        <form method="get">
          <select name="partner">
            <option value="">Partner</option>
            <% for option in partners { %>
            <option value="<%= option.uid %>"<% if let Some(partner) =  partner { %><% if option.uid.eq(partner) { %> selected<% } %><% } %>><%= option.name %></option>
            <% } %>
          </select>
          <select name="current">
            <option value="">Current or Not</option>
            <option value="true"<% if let Some(true) = current { %> selected<% } %>>Current</option>
            <option value="false"<% if let Some(false) = current { %> selected<% } %>>Not Current</option>
          </select>
          <select name="accepted">
            <option value="">Accepted or Not</option>
            <option value="true"<% if let Some(true) = accepted { %> selected<% } %>>Accepted</option>
            <option value="false"<% if let Some(false) = accepted { %> selected<% } %>>Not Accepted</option>
          </select>
          <select name="withdrawn">
            <option value="">Withdrawn or Not</option>
            <option value="true"<% if let Some(true) = withdrawn { %> selected<% } %>>Withdrawn</option>
            <option value="false"<% if let Some(false) = withdrawn { %> selected<% } %>>Not Withdrawn</option>
          </select>
          <input type="submit" value="Search">
          <span>(<%= num_matches %> matching opportunities)</span>
        </form>

        <div class="columns">
          <div class="column">
            <div class="field">
              <label class="label">UID</label>
              <div class="control">
                <%- health(1.0) %>
              </div>
            </div>
          </div>
          <div class="column">
            <div class="field">
              <label class="label">Created</label>
              <div class="control">
                <%- health(partner_created) %>
              </div>
            </div>
          </div>
          <div class="column">
            <div class="field">
              <label class="label">Updated</label>
              <div class="control">
                <%- health(partner_updated) %>
              </div>
            </div>
          </div>
        </div>

        <div class="field">
          <label class="label">Slug</label>
          <div class="control">
            <%- health(slug) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Partner Name</label>
          <div class="control">
            <%- health(partner_name) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Partner Website</label>
          <div class="control">
            <%- health(partner_website) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Partner Logo URL</label>
          <div class="control">
            <%- health(partner_logo_url) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Partner Opportunity URL</label>
          <div class="control">
            <%- health(partner_opp_url) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Organization Name</label>
          <div class="control">
            <%- health(organization_name) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Organization Type</label>
          <div class="control">
            <%- health(organization_type) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Organization Website</label>
          <div class="control">
            <%- health(organization_website) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Organization Logo URL</label>
          <div class="control">
            <%- health(organization_logo_url) %>
          </div>
        </div>

        <div class="field">
          <div class="columns">
            <div class="column">
              <label class="label">Opportunity Venue Types</label>
            </div>
            <div class="column">
            </div>
          </div>
          <div class="control">
            <%- health(opp_venue) %>
          </div>
        </div>

        <div class="field">
          <div class="columns">
            <div class="column">
              <label class="label">Opportunity Descriptors</label>
            </div>
            <div class="column">
            </div>
          </div>
          <div class="control">
            <%- health(opp_descriptor) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Minimum Age</label>
          <div class="control">
            <%- health(min_age) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Maximum Age</label>
          <div class="control">
            <%- health(max_age) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Domain (Public Engagement in Science)</label>
          <div class="control">
            <%- health(pes_domain) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Tags</label>
          <div class="control">
            <%- health(tags) %>
          </div>
        </div>

        <div class="field">
          <div class="columns">
            <div class="column">
              <label class="label">Opportunity Topics</label>
            </div>
            <div class="column">
            </div>
          </div>
          <div class="control">
            <%- health(opp_topics) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Ticket Required</label>
          <div class="control">
            <%- health(ticket_required) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Title</label>
          <div class="control">
            <%- health(title) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Description</label>
          <div class="control">
            <%- health(description) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Short Description (elevator pitch)</label>
          <div class="control">
            <%= short_desc %>
          </div>
        </div>

        <div class="field">
          <label class="label">Image URL</label>
          <div class="control">
            <%- health(image_url) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Image Credit</label>
          <div class="control">
            <%- health(image_credit) %>
          </div>
        </div>

        <div class="columns">
          <div class="column">
            <div class="field">
              <label class="label">Begins</label>
              <div class="control">
                <%- health(start_datetimes) %>
              </div>
            </div>
          </div>

          <div class="column">
            <div class="field">
              <label class="label">Ends</label>
              <div class="control">
                <%- health(end_datetimes) %>
              </div>
            </div>
          </div>

        </div>

        <div class="field">
          <label class="label">Daily Hours</label>
          <div class="control">
            <%- health(attraction_hours) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Has Cost</label>
          <div class="control">
            <%- health(cost) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Languages</label>
          <div class="control">
            <%- health(languages) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Hashtags</label>
          <div class="control">
            <%- health(opp_hashtags) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Social Handles</label>
          <div class="control">
            <%- health(opp_social_handles) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Is Online</label>
          <div class="control">
            <%- health(is_online) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Location Type</label>
          <div class="control">
            <%- health(location_type) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Point</label>
          <div class="control">
            <%- health(location_point) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Opportunity Geometry</label>
          <div class="control">
            <%- health(location_polygon) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Street</label>
          <div class="control">
            <%- health(address_street) %>
          </div>
        </div>

        <div class="field">
          <label class="label">City</label>
          <div class="control">
            <%- health(address_city) %>
          </div>
        </div>

        <div class="field">
          <label class="label">State/Province</label>
          <div class="control">
            <%- health(address_state) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Country</label>
          <div class="control">
            <%- health(address_country) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Zip</label>
          <div class="control">
            <%- health(address_zip) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Contact Name</label>
          <div class="control">
            <%- health(contact_name) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Contact Email</label>
          <div class="control">
            <%- health(contact_email) %>
          </div>
        </div>

        <div class="field">
          <label class="label">Contact Phone</label>
          <div class="control">
            <%- health(contact_phone) %>
          </div>
        </div>
      </div>
    </section>
  </body>
</html>
